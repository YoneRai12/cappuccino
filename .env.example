# .env のサンプルです。コピーして必要なトークンを入力してください
# 例: cp .env.example .env

# OpenAI API キー
OPENAI_API_KEY=your_openai_api_key_here
# APIエンドポイントを変更したい場合に指定します
OPENAI_API_BASE=https://api.openai.com/v1

# Azure OpenAI や Anthropic を利用する場合のキー
AZURE_OPENAI_API_KEY=your_azure_openai_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Discord ボット用トークン
DISCORD_BOT_TOKEN=your_discord_bot_token_here
DISCORD_TOKEN=your_discord_bot_token_here

# 気象情報取得用APIキー
OPENWEATHER_API_KEY=your_openweather_api_key_here

# Stable Diffusion Web UI を使う場合の設定
STABLE_DIFFUSION_API_KEY=your_stable_diffusion_api_key_here
STABLE_DIFFUSION_URL=http://localhost:7860

# ローカルLLMのモデルパス (任意)
LOCAL_MODEL_PATH=./models/model.bin

# Fractal Planner の探索深さと広さ
FRACTAL_DEPTH=2
FRACTAL_BREADTH=3

